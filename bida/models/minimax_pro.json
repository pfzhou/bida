{
    "model_type": "minimax-pro",
    "alias": "minimax-ccp, mm-ccp",
    "api_protocol": "minimax_pro_api:minimax_ccp_api",   
    "api_config":{  
        "api_key": {
            "default": "" ,
            "env":"minimax_api_key"
        },
        "group_id": {
            "default": "" ,
            "env":"minimax_groupID"
        }, 
        "chat_completions_baseurl": {
            "default": "https://api.minimax.chat/v1/text/chatcompletion_pro",
            "env":""
        },
        "embeddings_baseurl": {
            "default": "https://api.minimax.chat/v1/embeddings" ,
            "env":""
        }
    },
    "chat_completions": [
        {
            "name": "abab5.5-chat",
            "alias": "pro, 5.5",
            "is_default": true,
            "max_tokens": 16384,
            "description": "Chatcompletion pro默认版本，16K"
        }
    ],
    "embeddings": [
        {
            "name": "embo-01",
            "alias": "embedding",
            "is_default": true,
            "max_tokens": 4096,
            "description": "minimax的embedding模型，4K"
        }
    ]
}